@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>
<style>
.foto{
    background-image: url("https://www.comunilog.com/sites/default/files/condominio-1300x713.jpg");
    padding-top: 6rem;
     padding-bottom: 0rem;
}

</style>

<section class="mbr-section mbr-section-nopadding mbr-parallax-background mbr-after-navbar foto" id="msg-box4-1u" data-rv-view="24" >
    
    <div class="container">
        <div class="row">
            
            <div class="col-xs-12 col-lg-3 mbr-inner-padding text-xs-center mbr-table-cell-lg">
                <h3 class="mbr-section-title display-4">Users</h3>
                
                <div class="lead">As a MV company customer, we only hope you have the best experience</div>
                  
            </div>
              
            <div class="col-xs-12 col-lg-9 mbr-table-cell-lg image-size" style="width: 22%;">
                <div class="mbr-figure"><img src="~/css/assets/images/mbr-1618x1080.jpg"></div>
            </div>
        </div>
    </div>
</section>


<section class="mbr-section mbr-section-small" id="pricing-table1-1y" data-rv-view="74" style="background-color: rgba(255, 255, 255, 0.767); padding-top: 1.5rem; padding-bottom: 1.5rem;">
    

    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-3">
                <div class="card text-xs-center">
                    <div class="card-header bg-success">&nbsp;Meeting</div>
                 
                    <div><ul class="list-group list-group-flush"><li class="list-group-item"><strong>See all registered</strong></li></ul></div>
                    <div class="card-block"><a href="~/Meetings/Index" class="btn btn-primary-outline btn-primary">GO</a></div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3">
                <div class="card text-xs-center">
                    <div class="card-header bg-warning">Client</div>
                    
                    <div><ul class="list-group list-group-flush"><li class="list-group-item"><strong>See all registered</strong></li></ul></div>
                    <div class="card-block"><a  href= "~/Clients/Index "   class="btn btn-danger-outline btn-danger">GO</a></div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3">
                <div class="card text-xs-center">
                    <div class="card-header bg-danger">Tickets</div>
     
                    <div><ul class="list-group list-group-flush"><li class="list-group-item"><strong>See all registered</strong></li></ul></div>
                    <div class="card-block"><a href="~/Tickets/Index" class="btn btn-warning-outline btn-warning">GO</a></div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3">
                <div class="card text-xs-center">
                    <div class="card-header bg-primary">Contact</div>
               
                    <div><ul class="list-group list-group-flush"><li class="list-group-item"><strong>See all registered</strong></li></ul></div>
                    <div class="card-block"><a href="~/Contacts/Index" class="btn btn-success-outline btn-success">GO</a></div>
                </div>
            </div>
        </div>
    </div>
</section>
